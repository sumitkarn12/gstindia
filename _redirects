# Redirects from what the browser requests to what we serve

/					/user				301
/*.html				/user/index.html	200
/auth				/auth/index.html	200
/auth/*.html			/auth/index.html	200
/faq					/faq/index.html		200
/faq/*.html			/faq/index.html		200
/user					/user/index.html	200
/user/*.html			/user/index.html	200
/ca/					/ca/index.html		200
/ca/*.html				/ca/index.html		200
/admin				/admin/index.html	200
/admin/*.html			/admin/index.html	200
